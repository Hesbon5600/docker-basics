{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "app",
      "image": "hesbon5600/docker-basics-app-image:1.0",
      "hostname": "app",
      "essential": false,
      "memory": 128,
      "links": ["database"]
    },
    {
      "name": "database",
      "image": "postgres:10.1",
      "essential": false,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 5432,
          "containerPort": 5432
        }
      ]
    },
    {
      "name": "proxy",
      "image": "hesbon5600/docker-basics-nginx-custom:1.0",
      "hostname": "proxy",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["app"]
    }
  ]
}